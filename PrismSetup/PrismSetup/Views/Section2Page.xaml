<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PrismSetup.Views.Section2Page"
             xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
             prism:ViewModelLocator.AutowireViewModel="True">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Page 2" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>

            <Button Text="Go back page 1" 
                    Command="{Binding NavigateBackPage1Command}"
                    HorizontalOptions="CenterAndExpand" 
                    VerticalOptions="EndAndExpand"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>